{
  "moduleNameMapper": {
    "^@lib/compiler/template$": "<rootDir>/compiler/index.js",
    "^@lib/(.*)$": "<rootDir>/dist/es/index.min.js",
    "^@test/(.*)$": "<rootDir>/tests/$1"
  },
  "transform": {
    "^.+\\.(ts|js)$": "babel-jest"
  },
  "testRegex": "(/tests/.*)(test|spec)\\.ts$",
  "moduleFileExtensions": [
    "js",
    "ts"
  ]
}