{
  "moduleNameMapper": {
    "^@lib/template/template$": "<rootDir>/template/index.js",
    "^@lib/(.*)$": "<rootDir>/dist/esm/index.min.js",
    "^@test/(.*)$": "<rootDir>/tests/$1"
  },
  "transform": {
    "^.+\\.(ts|js)$": "babel-jest"
  },
  "testRegex": "(/tests/.*)(test|spec)\\.ts$",
  "moduleFileExtensions": [
    "js",
    "ts"
  ]
}